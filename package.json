{
  "name": "rc-controller",
  "version": "1.0.0",
  "description": "Control your RC Model from your Browser!",
  "main": "index.js",
  "scripts": {
    "static": "jshint src/js --show-non-errors",
    "test": "cross-env NODE_ENV=test nyc -a --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "analyze": "jshint --verbose --show-non-errors ./src",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichalSchwarz/rc-controller.git"
  },
  "keywords": [
    "rc",
    "model",
    "controller"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/js/class/**/*.js"
    ],
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "jshintConfig": {
    "expr": true,
    "curly": true,
    "eqeqeq": true,
    "esversion": 6,
    "globals": {
      "XMLHttpRequest": true
    }
  },
  "author": "Michal Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichalSchwarz/rc-controller/issues"
  },
  "homepage": "https://github.com/MichalSchwarz/rc-controller#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jshint": "^2.9.5",
    "json-loader": "^0.5.7",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "nyc": "^11.7.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglify-js": "^3.3.24",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.1"
  }
}
