{
  "name": "rc-controller",
  "version": "1.0.0",
  "description": "Control your RC Model from your Browser!",
  "main": "index.js",
  "scripts": {
    "static": "jshint src/js --show-non-errors",
    "test": "cross-env NODE_ENV=test nyc -a mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichalSchwarz/rc-controller.git"
  },
  "keywords": [
    "rc",
    "model",
    "controller"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/js/class/**/*.js"
    ],
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "jshintConfig": {
    "expr": true,
    "curly": true,
    "eqeqeq": true,
    "esversion": 6
  },
  "author": "Michal Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichalSchwarz/rc-controller/issues"
  },
  "homepage": "https://github.com/MichalSchwarz/rc-controller#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-inline-source-plugin": "0.0.8",
    "html-webpack-plugin": "^2.28.0",
    "jshint": "^2.9.4",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mock-browser": "^0.92.14",
    "node-sass": "^4.5.2",
    "nyc": "^10.2.0",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.2",
    "webpack": "^2.3.3"
  }
}
